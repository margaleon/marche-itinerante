<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <title>Marche itinérante | Error</title>
    
    <link rel="stylesheet" href="/static/css/styles.css">
    <link href="static/img/favicon.ico" rel="shortcut icon" type="image/x-icon" />

    <script src="https://kit.fontawesome.com/2f8670a614.js" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  </head>
  <body>

    <!-- header -->
    <header class="header">
      <a class="home-btn" href="/">accueil</a>
    </header>

    <!-- error section -->
    <section class="error-section" id="error" title="error">
      <h2>Oups !</h2>
      <h3><%= message %></h3>
      <a href="/" class="return-btn btn">Retour à l'accueil</a>
    </section>

    <!-- footer -->
    <footer class="footer" id="footer">
      <%- include("./partials/footer"); %>
    </footer>
  </body>


</html>

